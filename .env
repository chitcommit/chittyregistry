# ChittyRegistry Production Configuration
PORT=3000
NODE_ENV=production

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_DB=0

# ChittyID Integration
CHITTYID_URL=https://id.chitty.cc
CHITTYID_CLIENT_ID=chittyregistry
CHITTYID_CLIENT_SECRET=prod_secret_placeholder

# Service Configuration
SERVICE_NAME=chittyregistry
SERVICE_VERSION=1.0.0
SERVICE_URL=https://registry.chitty.cc

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000
HEALTH_CHECK_RETRIES=3

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# CORS
CORS_ORIGINS=https://chitty.cc,https://*.chitty.cc

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100